<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_partner_form_inherited_cap" model="ir.ui.view">
        <field name="name">res.partner.form.inherited.cap</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='category_id']" position="after">
                <!-- <field name="cohort_date"/> -->
                <field name="registration_complete"/>
                <field name="visit_started"/>
                <field name="screening_complete"/>
                <field name="physical_complete"/>
                <field name="donation_complete"/>
                <field name="multiple_donations_complete"/>
                <field name="boolean_updated" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>
