<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <!-- Modified Partner Form: -->
    <record id="res_partner_customizations" model="ir.ui.view">
        <field name="name">res.partner.customizations</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='lang']" position="after">
                <field name="latest_visited_date"/>
                <field name="x_latest_visited_date_new" />
            </xpath>
        </field>
    </record>
  </data>
</odoo>