<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--print_excel_report method is called from server action-->
    <record id="action_new_donor_report" model="ir.actions.server">
        <field name="name">Link Count Report</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="model_link_tracker"/>
        <field name="state">code</field>
        <field name="code">
            action = model.print_excel_report()
        </field>
    </record>

    <menuitem id="menu_report_new_donor" name="New Donor Aquisition"
        action="action_new_donor_report"
        parent="website_sale.menu_reporting" sequence="2"/>

</odoo>